<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.nero.click.data.dao.BeesDao">

    <select id="getGeneId" resultType="Integer">
        SELECT geneID from gene where geneName in
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">
             #{item}
        </foreach>
    </select>
    <select id="getPropertyId" resultType="Integer">
        select propertyID FROM relation WHERE ${fuckcid} IN
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">
          #{item}
        </foreach>
    </select>
    <select id="getBeesPoints" resultType="org.nero.click.data.dto.Point">
        SELECT x,y,sampleID from ${table} where propertyID IN
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>


    <!--新的mapper-->

    <select id="getTables" resultType="String">
        show tables
    </select>
    <select id="getColumns" resultType="String">
        desc ${cancerType}_${normal}_${dataType}_${value}_${index}_${page};
    </select>

    <select id="getGenesId" resultType="Integer">
        SELECT ID from ${dataType}_gene where genename in
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">
            '${item}'
        </foreach>
    </select>
    <select id="getData" resultType="org.nero.click.data.dto.Point">
        SELECT * from  ${cancerType}_${normal}_${dataType}_${value}_${index}_${page} where geneid IN
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">
            ${item}
        </foreach>
    </select>


</mapper>