<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.nero.click.dao.BeesDao">

    <select id="getGeneId" resultType="Integer">
        SELECT geneID from gene where geneName in
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">
             #{item}
        </foreach>
    </select>
    <select id="getPropertyId" resultType="Integer">
        select propertyID FROM relation WHERE ${fuckcid} IN
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">
          #{item}
        </foreach>
    </select>
    <select id="getBeesPoints" resultType="org.nero.click.dto.Point">
        SELECT x,y,sampleID from ${table} where propertyID IN
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>
    <select id="getBox" resultType="org.nero.click.dao.Box">
        SELECT
        <foreach collection="list" index="index" item="item" separator=",">
            #{item}
        </foreach>
        from gene where geneName in
        <foreach collection="list" index="index" item="item" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>

</mapper>